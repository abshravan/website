{
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#f5f6fc",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#616161",
					"name": "Body Text",
					"slug": "body-text"
				},
				{
					"color": "#f8f8f8",
					"name": "Section Background",
					"slug": "section-bg"
				},
				{
					"color": "#000000",
					"name": "Headings",
					"slug": "heading"
				},
				{
					"color": "#C03320",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#111111",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "999px",
			"wideSize": "1170px"
		},
		"spacing": {
			"units": [
				"px",
				"%",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Sarabun",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Sarabun-Regular.ttf"
							]
						},
						{
							"fontFamily": "Sarabun",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/Sarabun-Medium.ttf"
							]
						},
						{
							"fontFamily": "Sarabun",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/Sarabun-SemiBold.ttf"
							]
						},
						{
							"fontFamily": "Sarabun",
							"fontStretch": "",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Sarabun-Bold.ttf"
							]
						}
					],
					"fontFamily": "\"Sarabun\", sans-serif",
					"name": "Sarabun",
					"slug": "sarabun"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "14px",
					"slug": "small"
				},
				{
					"name": "Body Text",
					"size": "16px",
					"slug": "body-text"
				},
				{
					"name": "Medium",
					"size": "18px",
					"slug": "medium"
				},
				{
					"name": "Regular",
					"size": "22px",
					"slug": "regular"
				},
				{
					"name": "Large",
					"size": "26px",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "32px",
					"slug": "extra-large"
				},
				{
					"name": "Big",
					"size": "clamp(2.063rem, 1.759rem + 1.518vw, 3.125rem)",
					"slug": "big"
				},
				{
					"name": "Huge",
					"size": "clamp(40px, calc(2.25rem + ((1vw - 3.2px) * 2.375)), 74px);",
					"slug": "huge"
				},
				{
					"name": "Gigantic",
					"size": "clamp(42px, calc(2.625rem + ((1vw - 3.2px) * 2.75)), 86px);",
					"slug": "gigantic"
				},
				{
					"name": "Banner Title",
					"size": "clamp(2.5rem, 0.938rem + 4.167vw, 4.688rem)",
					"slug": "banner-title"
				}
			],
			"fontWeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--white);"
				},
				"elements": {
					"link": {
						":active": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary);"
							}
						}
					}
				},
				"typography": {
					"fontWeight": "600",
					"letterSpacing": "2px"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary);"
						},
						"typography": {
							"textDecoration": "none",
							"textTransform": "uppercase"
						}
					}
				},
				"typography": {
					"fontSize": "clamp(1.875rem, 1.661rem + 1.071vw, 2.625rem);",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			}
		},
		"color": {
			"background": "#0c3b2e",
			"text": "var:preset|color|base"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--white)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var:preset|color|luminous-vivid-amber"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "40px",
					"lineHeight": "1.5"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "30px",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "24px"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "22px"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "18px"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "16px"
				}
			},
			"heading": {
				"color": {
					"text": "#000000"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--sarabun)",
					"fontWeight": "600"
				}
			},
			"link": {
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"outline": {
						"style": "dotted",
						"width": "1px"
					}
				},
				":hover": {
					"color": {
						"text": "var:preset|color|base"
					}
				},
				"color": {
					"text": "var:preset|color|body-text"
				}
			}
		},
		"spacing": {
			"margin": {
				"bottom": "0",
				"left": "0",
				"right": "0",
				"top": "0"
			},
			"padding": {
				"bottom": "0",
				"left": "0",
				"right": "0",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sarabun)",
			"fontSize": "var(--wp--preset--font-size--body-text)",
			"fontWeight": "400",
			"lineHeight": "1.5rem"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"title": "Lite",
	"version": 2
}